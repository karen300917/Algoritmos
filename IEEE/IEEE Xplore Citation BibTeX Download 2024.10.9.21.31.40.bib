  doi={10.1109/ISPA.2019.8868473},
  ISSN={1849-2266},
  month={Sep.},}@INPROCEEDINGS{6919190,
  author={Evripidou, Paraskevas and Kyriacou, Costas},
  booktitle={2013 Data-Flow Execution Models for Extreme Scale Computing}, 
  title={Data-Flow vs Control-Flow for Extreme Level Computing}, 
  year={2013},
  volume={},
  number={},
  pages={9-13},
  abstract={This paper challenges the current thinking for building High Performance Computing (HPC) Systems, which is currently based on the sequential computing also known as the von Neumann model, by proposing the use of Novel systems based on the Dynamic Data-Flow model of computation. The switch to Multi-core chips has brought the Parallel Processing into the mainstream. The computing industry and research community were forced to do this switch because they hit the Power and Memory walls. Will the same happen with HPC? The United States through its DARPA agency commissioned a study in 2007 to determine what kind of technologies will be needed to build an Exaflop computer. The head of the study was very pessimistic about the possibility of having an Exaflop computer in the foreseeable future. We believe that many of the findings that caused the pessimistic outlook were due to the limitations of the sequential model. A paradigm shift might be needed in order to achieve the affordable Exascale class Supercomputers.},
  keywords={Instruction sets;Context;Computational modeling;Synchronization;Parallel processing;Runtime;Supercomputers;Data-Flow;HPC;Exascale;Supercomputing},
  doi={10.1109/DFM.2013.17},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{9737246,
  author={Zhao, Da-Sheng and Song, Jing-Jing and Xu, Tai-Hua and Tsang, Eric C.C.},
  booktitle={2021 International Conference on Machine Learning and Cybernetics (ICMLC)}, 
  title={Accelerator on Multi-Granularity Attribute Reduction for Continuous Parameters}, 
  year={2021},
  volume={},
  number={},
  pages={1-6},
  abstract={Currently, in the field of Granular Computing, parameterized granularity has been widely explored by many scholars. Notably, the multi-granularity model for continuous parameters has also been attracted extensive attention. Based on such a model, it is often necessary to consider multiple granularities to evaluate the significance of candidate condition attributes in solving problems oriented to attribute reduction, which may lead to high time consumption. In view of this, one acceleration algorithm based on multi-granularity attribute reduction (MGAR) for continuous parameters is designed to decrease the time consumption of acquiring reduct. In fact, the acceleration method we proposed is on the basis of the thinking of positive approximation. Specifically, the iterations of evaluating and selecting condition attributes are carried out on the progressively decreased universe instead of the entire sample space. The experimental results over 10 UCI datasets show that the acceleration algorithm not only can effectively accelerate the process of searching reduct, but also can generate the same reduct with traditional approach.},
  keywords={Machine learning algorithms;Granular computing;Computational modeling;Machine learning;Approximation algorithms;Cybernetics;Attribute reduction;Multi-granularity;Continuous parameters;Neighborhood rough set;Granular computing},
