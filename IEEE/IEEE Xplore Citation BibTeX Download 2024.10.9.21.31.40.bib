  ISSN={2155-1812},
  month={June},}@INPROCEEDINGS{5429167,
  author={Ehm, Hans and McGinnis, Leon and Rose, Oliver},
  booktitle={Proceedings of the 2009 Winter Simulation Conference (WSC)}, 
