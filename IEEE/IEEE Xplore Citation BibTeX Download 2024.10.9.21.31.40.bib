  abstract={This paper deals with an adapted relation structure (a-RS) algorithm for WSD. In the previous research, we confirmed that the volume of knowledge base does not affect to the performance especially, on precision of WSD. We think the precision is more important than recall rate because WSD is core for other semantic information processing fields and relates to the results deeply. Therefore, we propose an adapted method to improve the precision based on relation structure (RS) algorithm. Through experiment comparing 3 kinds (SSI, RS, and a-RS) of WSD method, we show the performance of a-RS algorithm.},
  keywords={Laboratories;Buildings;Dictionaries;Intelligent structures;Information processing;Computational linguistics;Information retrieval;Ontologies;Taxonomy;Indexing},
  doi={10.1109/ICDIM.2008.4746825},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{9225977,
  author={Gao, Yayu and Zhang, Chengwei and Hei, Xiaojun and Zhong, Guohui},
  booktitle={2019 IEEE International Conference on Engineering, Technology and Education (TALE)}, 
