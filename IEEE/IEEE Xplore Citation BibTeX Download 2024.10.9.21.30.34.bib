  abstract={Pervasive computing, as one of the real "big definition" in information technology, has basically changed people's thinking towards information technology and the way we live and work, and the Internet revolution resulted from a grid will change the setup of the whole computer area. This article introduces the definitions, the technology characteristics of pervasive computing technology and network technology and the relationships between the two. In order to solve the disadvantages of the present tele-education system, with the help of grid and pervasive computing technology, this article builds a real-time distance education system framework (PETF), and describes the achieved functions and features of this framework},
  keywords={Pervasive computing;Education;Grid computing;Embedded computing;Application software;Educational technology;Humans;Computer networks;Information technology;Distance learning;Pervasive computing;PETF;Grid;Tele-education},
  doi={10.1109/SPCA.2006.297440},
  ISSN={},
  month={Aug},}@INPROCEEDINGS{6044253,
  author={Muha, Damir and Hrženjak, Nevena and Hrabar, Silvio and Zaluški, Davor},
  booktitle={Proceedings ELMAR-2011}, 
  title={An RF analog of ENZ dielectric waveguide}, 
  year={2011},
  volume={},
  number={},
  pages={377-380},
  abstract={Recently, there have been several studies on potential application of the ENZ (plasmonic) metamaterials in dielectric waveguides and similar structures at optical frequencies. It was shown possible to tunnel the flow of EM energy trough a slab (or a rod) made out of an ENZ material, transversal dimension of which can be arbitrarily smaller than a wavelength. In addition, one could think of a complementary structure that comprises a dielectric slab embedded within an ENZ metamaterial. Such a structure (a simple air slab with a subwavelength thickness, embedded within an anisotropic ENZ metamaterial) is analyzed in this paper. Numerical results revealed behavior similar to the behavior of an ordinary dielectric waveguide, but with considerably longer guiding wavelength (for a fixed width of a core slab). Numerical results were verified by the measurements on the RF scaled replica of an ENZ waveguide, operating in 4 GHz frequency band.},
  keywords={Dielectrics;Slabs;Optical waveguides;Permittivity;Metamaterials;Radio frequency;ENZ;dielectric waveguide;metamaterial},
  doi={},
  ISSN={1334-2630},
  month={Sep.},}@INPROCEEDINGS{7834037,
