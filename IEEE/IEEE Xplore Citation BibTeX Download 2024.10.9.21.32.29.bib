  keywords={Training;Systematics;Object oriented modeling;Computational modeling;Employment;Data models;Software;Python Structure;Computer-based Simulation;Data Model;Management Information System;Intelligent Training},
  doi={10.1109/ICSSIT53264.2022.9716516},
  ISSN={},
  month={Jan},}@INPROCEEDINGS{9477735,
