  keywords={Fuzzy systems;Information retrieval;Knowledge engineering;Medical diagnosis;Computer displays;Marine vehicles;Biomedical engineering;Design engineering;Systems engineering and theory;Thesauri},
  doi={10.1109/FUZZY.1998.687481},
