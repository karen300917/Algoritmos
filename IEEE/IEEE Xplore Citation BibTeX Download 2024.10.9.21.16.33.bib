  booktitle={2018 IEEE East-West Design & Test Symposium (EWDTS)}, 
  title={Cyber Social Computing}, 
  year={2018},
  volume={},
  number={},
  pages={1-8},
  abstract={A cyberculture of social computing is proposed that integrates technologies of exhaustive human-free metric monitoring and digital management of social processes and phenomena. Logical structures of cyber social computing are defined, which are considered as components of cyber technologies for accurate monitoring and moral digital management of society. The types of emotionally logical computing, the metrics and algorithms for measuring social relations along horizontal and vertical communications, the rules of human behavior, focused on the creation of emotional logic for modeling and predicting people's behavior. A cyber-physical model of digital statehood is introduced for metric management of resources and citizens based on exhaustive monitoring of people's needs, including components of social computing (attitudes, goals, management, personnel, infrastructure, resources) ordered by their impact on market success. The cyber social architecture of the moral digital management of science and resources is offered based on exhaustive metric monitoring of educational processes.},
  keywords={Measurement;Ethics;Social computing;Monitoring;Companies;Computational modeling},
  doi={10.1109/EWDTS.2018.8524663},
  ISSN={2472-761X},
  month={Sep.},}@INPROCEEDINGS{8919756,
  author={Vamvoudakis, Kyriakos G. and Kanellopoulos, Aris},
  booktitle={2019 57th Annual Allerton Conference on Communication, Control, and Computing (Allerton)}, 
  title={Non-Equilibrium Learning and Cyber-Physical Security}, 
  year={2019},
  volume={},
  number={},
  pages={1-6},
  abstract={This paper introduces a framework for non-equilibrium behavior analysis in cyber-physical systems for security purposes. To categorize the player, we employ the principles of reinforcement learning in order to derive an iterative method of optimal responses that determine the policy of an agent with level-k intelligence in a general non-zerosum, nonlinear environment. For the special case of zero-sum, linear quadratic games we derive appropriate non-equilibrium game Riccati equations. To obviate the need for complete knowledge of the system dynamics, we employ a Q-learning algorithm as a best response solver. We then design an estimator that determines the distribution of intelligence levels in the adversarial environment of the system. Finally, simulation results showcase the efficacy of our approach.},
